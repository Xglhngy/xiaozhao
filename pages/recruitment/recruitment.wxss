.wrapper{
  width: 100%;
  background: #f0f0f0;
}
.listdata{
  width: 100%;
  height: 160rpx;
  margin-bottom:4rpx;
  background: white;
}
.zw,.js{
  width: 100%;
  height: 80rpx;
  padding-left: 50rpx;
  padding-right: 50rpx;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  box-sizing: border-box;
}
.zw{
  line-height: 100rpx;
  font-size: 40rpx;
  color:black;
  /* font-weight: bold; */
}
.js{
  line-height: 70rpx;
  font-size: 30rpx;
  color:#999;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.dz{
  width:60%;
  height: 50rpx;
  line-height: 50rpx;
  overflow: hidden;
  display: flex;
  flex-direction: row;
  justify-content: left;
  align-items: center;
}
.date{
  width:30%;
  height: 35rpx;
  line-height: 35rpx;
  overflow: hidden;
}
.address{
  display: inline-block;
  width: 200rpx;
  height: 35rpx;
  line-height: 35rpx;
  overflow: hidden;
}
.education,.number{
  display: inline-block;
  height: 35rpx;
  line-height: 35rpx;
  overflow: hidden;
}

